{
  "components/product/images": "<div class=\"product-images\" data-product-images>\n  <div class=\"product-image-slides-wrap\">\n    <div\n      class=\"product-image-slides\"\n      data-slideshow-position=\"0\"\n      data-product-slides\n    >\n      {{#if product.images}}\n        {{#each product.images}}\n          <a\n            class=\"product-image-slide\"\n            href=\"{{getImage this 'banner'}}\"\n            data-image-position=\"{{@index}}\"\n            data-product-image\n            data-flickity-lazyload-srcset=\"{{getImage this 'large'}}\"\n            data-flickity-lazyload-src=\"{{getImage this 'large'}}\"\n          >\n            <img src=\"{{getImage this 'large'}}\" alt=\"{{alt}}\">\n          </a>\n        {{/each}}\n      {{else}}\n        <img\n          class=\"product-image-slide\"\n          src=\"{{cdn theme_settings.default_image}}\"\n          alt=\"{{lang 'common.no_image'}}\"\n          data-product-image\n        >\n      {{/if}}\n    </div>\n    <div\n      class=\"product-images-pagination\"\n      data-image-position=\"{{@index}}\"\n      data-product-thumbnails\n    >\n      {{#each product.images}}\n        <span\n          class=\"product-images-pagination-item {{#if @first}}active{{/if}}\"\n          style=\"background-image: url('{{getImage this 'thumb'}}');\"\n          data-image-position=\"{{@index}}\"\n          data-product-thumbnails-slide\n        >\n          <img\n            class=\"sr-only\"\n            src=\"{{getImage this 'thumb'}}\"\n            alt=\"{{this.alt}}\"\n          >\n        </span>\n      {{/each}}\n    </div>\n  </div>\n</div>\n"
}
