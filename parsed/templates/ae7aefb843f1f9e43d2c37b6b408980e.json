  "components/common/share-links": "<ul class=\"share-links\">\n  {{#each share_buttons.buttons}}\n\n    {{#if service '===' 'facebook'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          href=\"//www.facebook.com/sharer.php?u={{../../url}}\"\n          class=\"share-facebook\"\n          data-share\n        >\n          <span class=\"sr-only\">Facebook</span>\n          {{> svg/icon-facebook}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'twitter'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          href=\"//twitter.com/share?url={{../../url}}\"\n          class=\"share-twitter\"\n          data-share\n        >\n          <span class=\"sr-only\">Twitter</span>\n          {{> svg/icon-twitter}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'pinterest'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          class=\"share-pinterest\"\n          href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\"\n          data-share\n        >\n          <span class=\"sr-only\">Pinterest</span>\n          {{> svg/icon-pinterest}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'linkedin'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          href=\"//www.linkedin.com/cws/share?url={{../../url}}\"\n          class=\"share-linkedin\"\n          data-share\n        >\n          <span class=\"sr-only\">LinkedIn</span>\n          {{> svg/icon-linkedin}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'google_plusone'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          href=\"//plus.google.com/share?url={{../../url}}\"\n          class=\"share-google\"\n          data-share\n        >\n          <span class=\"sr-only\">Google Plus</span>\n          {{> svg/icon-google-plus}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'email'}}\n      <li class=\"share-link\">\n        <a\n          href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\"\n          class=\"share-email\"\n        >\n          <span class=\"sr-only\">{{lang 'common.email'}}</span>\n          {{> svg/icon-mail}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'print'}}\n      <li class=\"share-link\">\n        <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\n          <span class=\"sr-only\">{{lang 'common.print'}}</span>\n          {{> svg/icon-print}}\n        </a>\n      </li>\n    {{/if}}\n\n  {{/each}}\n\n  {{#each share_buttons.buttons}}\n    {{#if service '===' 'facebook_like'}}\n      <li class=\"share-link facebook-like\">\n        <div class=\"fb-like\"\n          data-href=\"{{../../url}}\"\n          data-layout=\"button_count\"\n          data-action=\"like\"\n          data-show-faces=\"false\">\n        </div>\n      </li>\n    {{/if}}\n  {{/each}}\n</ul>\n",
