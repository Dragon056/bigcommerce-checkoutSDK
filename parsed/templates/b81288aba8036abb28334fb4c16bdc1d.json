{
  "components/product-catalog/compare-widget": "{{!--\n  Compare Widget - displays compare items on any page\n--}}\n\n<aside class=\"compare-widget\" data-compare-widget data-compare-count=\"0\" aria-hidden=\"true\">\n  <header class=\"compare-widget-header\">\n    <button class=\"compare-widget-toggle\">\n      {{> svg/chevron-up-flat}}\n      <span class=\"compare-one\">{{lang 'common.compare.compare_one'}}</span>\n      <span class=\"compare-multiple\">\n        <span class=\"compare-count\"></span>\n        {{lang 'common.compare.compare_multiple'}}\n      </span>\n    </button>\n    <span class=\"compare-widget-cta\">\n      {{lang 'common.compare.compare_widget_cta'}}\n    </span>\n    <button class=\"compare-widget-remove-all\" data-compare-remove-all>\n      {{lang 'common.compare.remove_all'}}\n    </button>\n  </header>\n  <div class=\"compare-widget-content\">\n    <div class=\"compare-widget-items-wrap\">\n      <div class=\"compare-widget-items\" data-compare-items>\n        {{! Compare items injected here }}\n      </div>\n    </div>\n\n    <footer class=\"compare-widget-footer\">\n      <button class=\"compare-widget-cancel\" data-compare-remove-all>\n        {{lang 'common.compare.cancel'}}\n      </button>\n      <a class=\"compare-widget-action\" href=\"{{urls.compare}}\" data-compare-link=\"{{urls.compare}}\">\n        {{lang 'core.product.compare'}}\n      </a>\n    </footer>\n  </div>\n</aside>\n",
  "svg/chevron-up-flat": "<svg width=\"11\" height=\"4\" viewBox=\"0 0 11 4\" xmlns=\"http://www.w3.org/2000/svg\" role=\"presentation\">\n  <path d=\"M.704 4L0 3.53 5.352 0l5.352 3.53L10 4 5.352.936\" fill-rule=\"nonzero\" fill=\"currentColor\"/>\n</svg>\n"
}
