{
  "components/cart/shipping-calculator": "<button class=\"modal-close\">\n  {{> svg/icon-x}}\n</button>\n<div class=\"shipping-calculator cart-modal-content\" data-shipping-calculator>\n\n  {{> components/common/page-title title=(lang 'cart.shipping_calculator.title')}}\n\n  {{!-- Select Destination Form --}}\n  <form class=\"form\">\n    <div class=\"form-field form-field-select form-required\">\n      <label class=\"form-label\" for=\"shipping-country\">\n        <span class=\"form-field-title\">\n          {{lang 'cart.shipping_calculator.select_a_country'}}\n          <span class=\"required-text\">{{lang 'common.required'}}</span>\n        </span>\n        <span class=\"form-field-control\">\n          <span class=\"form-select-wrapper\">\n            <select\n              class=\"form-select form-input\"\n              id=\"shipping-country\"\n              name=\"shipping-country\"\n              data-field-type=\"Country\"\n            >\n              <option>{{lang 'cart.shipping_calculator.select_a_country'}}</option>\n              {{#each countries}}\n                <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                  {{name}}\n                </option>\n              {{/each}}\n            </select>\n          </span>\n        </span>\n      </label>\n    </div>\n\n    <div class=\"form-field form-field-select form-required\">\n      <label class=\"form-label\" for=\"shipping-state\">\n        <span class=\"form-field-title\">\n          {{lang 'cart.shipping_calculator.select_a_state'}}\n          <span class=\"required-text\">{{lang 'common.required'}}</span>\n        </span>\n        <span class=\"form-field-control\">\n          <span class=\"form-select-wrapper\">\n            <select\n              class=\"form-select form-input\"\n              id=\"shipping-state\"\n              name=\"shipping-state\"\n              data-field-type=\"State\"\n            >\n              <option>{{lang 'cart.shipping_calculator.select_a_state'}}</option>\n              {{#each states}}\n                <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                  {{name}}\n                </option>\n              {{/each}}\n            </select>\n          </span>\n        </span>\n      </label>\n    </div>\n\n    <div class=\"form-field\">\n      <label class=\"form-label\" for=\"shipping-city\">\n        <span class=\"form-field-title\">\n          {{lang 'cart.shipping_calculator.suburb_city'}}\n        </span>\n        <input\n          class=\"form-input\"\n          type=\"text\"\n          id=\"shipping-city\"\n          name=\"shipping-city\"\n          value=\"{{selected_city}}\"\n        >\n      </label>\n    </div>\n\n    {{> core/forms/common/text\n      label=(lang 'cart.shipping_calculator.zip_postal_code')\n      name=\"shipping-zip\"\n      id=\"shipping-zip\"\n    }}\n\n    <div data-shipping-errors>\n      {{!-- Alert placeholder --}}\n    </div>\n\n    <div class=\"form-actions\">\n      <button class=\"button button-secondary\">\n        {{lang 'cart.shipping_calculator.estimate_shipping'}}\n      </button>\n    </div>\n  </form>\n\n  <div data-shipping-quotes class=\"shipping-quotes\">\n    {{!-- Quotes placeholder --}}\n  </div>\n</div>\n",
  "svg/icon-x": "<svg class=\"icon-x\" width=\"11\" height=\"11\" viewBox=\"0 0 11 11\" xmlns=\"http://www.w3.org/2000/svg\" role=\"presentation\">\n  <path d=\"M.367.933l9.2 9.2c.156.156.41.156.566 0 .156-.156.156-.41 0-.566l-9.2-9.2C.777.21.523.21.367.367.21.523.21.777.367.933zm9.2-.566l-9.2 9.2c-.156.156-.156.41 0 .566.156.156.41.156.566 0l9.2-9.2c.156-.156.156-.41 0-.566-.156-.156-.41-.156-.566 0z\" fill-rule=\"nonzero\" fill=\"currentColor\"/>\n</svg>\n",
  "components/common/page-title": "<h1 class=\"page-title\">{{title}}</h1>\n",
  "core/forms/common/text": "\n<div class=\"form-field form-field-common form-field-text {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n"
}
