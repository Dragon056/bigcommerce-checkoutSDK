{
  "components/common/share-widget": "{{!--\n  Share Links - Displayed on Blog Posts and Product Pages\n\n  @param url\n    The URL of the post that should be shared to social media.\n\n  @param share_buttons\n    The data for the buttons to be shared from Bigcommerce\n\n  @param share_type (optional)\n    Added as a class name to the share-links-container\n\n  @param image\n    The image to add to the pinterest post\n--}}\n\n{{#if settings.add_this.buttons.length}}\n  <div class=\"dropdown product-share-block\" data-dropdown>\n    <button class=\"dropdow-toggle share-dropdown-toggle\" data-dropdown-toggle>\n      <span class=\"share-icon\">\n        {{> svg/icon-share}}\n      </span>\n      {{ lang 'product.share_item'}}\n    </button>\n    <div class=\"dropdown-panel share-dropdown-panel\" data-dropdown-panel>\n      {{> components/common/share-links\n        url=product.url\n        title=product.title\n        share_type='product-post'\n        share_buttons=settings.add_this\n        image=(getImage product.main_image 'share')\n      }}\n    </div>\n  </div>\n{{/if}}\n",
  "svg/icon-share": "<svg width=\"18\" height=\"18\" viewBox=\"0 0 18 18\" xmlns=\"http://www.w3.org/2000/svg\" role=\"presentation\">\n  <path stroke-width=\".5\" d=\"M12.4 1c-1.432 0-2.6 1.198-2.6 2.667 0 .277.039.546.116.797l-3.61 2.53a2.552 2.552 0 0 0-1.706-.66C3.168 6.333 2 7.531 2 9s1.168 2.667 2.6 2.667c.652 0 1.249-.252 1.706-.661l3.616 2.524a2.71 2.71 0 0 0-.122.803C9.8 15.802 10.968 17 12.4 17c1.432 0 2.6-1.198 2.6-2.667 0-1.468-1.168-2.666-2.6-2.666-.895 0-1.685.469-2.153 1.178L6.794 10.43a2.696 2.696 0 0 0-.006-2.858l3.459-2.416A2.583 2.583 0 0 0 12.4 6.333c1.432 0 2.6-1.198 2.6-2.666C15 2.198 13.832 1 12.4 1zm0 .889c.961 0 1.733.792 1.733 1.778s-.772 1.777-1.733 1.777c-.961 0-1.733-.791-1.733-1.777 0-.986.772-1.778 1.733-1.778zM4.6 7.222c.961 0 1.733.792 1.733 1.778s-.772 1.778-1.733 1.778c-.961 0-1.733-.792-1.733-1.778S3.639 7.222 4.6 7.222zm7.8 5.334c.961 0 1.733.791 1.733 1.777 0 .986-.772 1.778-1.733 1.778-.961 0-1.733-.792-1.733-1.778s.772-1.777 1.733-1.777z\" fill=\"currentColor\" fill-rule=\"evenodd\" stroke=\"currentColor\"/>\n</svg>\n",
  "components/common/share-links": "<ul class=\"share-links\">\n  {{#each share_buttons.buttons}}\n\n    {{#if service '===' 'facebook'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          href=\"//www.facebook.com/sharer.php?u={{../../url}}\"\n          class=\"share-facebook\"\n          data-share\n        >\n          <span class=\"sr-only\">Facebook</span>\n          {{> svg/icon-facebook}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'twitter'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          href=\"//twitter.com/share?url={{../../url}}\"\n          class=\"share-twitter\"\n          data-share\n        >\n          <span class=\"sr-only\">Twitter</span>\n          {{> svg/icon-twitter}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'pinterest'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          class=\"share-pinterest\"\n          href=\"//pinterest.com/pin/create/button/?url={{../../url}}&amp;media={{../../image}}&amp;description={{../../title}}\"\n          data-share\n        >\n          <span class=\"sr-only\">Pinterest</span>\n          {{> svg/icon-pinterest}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'linkedin'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          href=\"//www.linkedin.com/cws/share?url={{../../url}}\"\n          class=\"share-linkedin\"\n          data-share\n        >\n          <span class=\"sr-only\">LinkedIn</span>\n          {{> svg/icon-linkedin}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'google_plusone'}}\n      <li class=\"share-link\">\n        <a\n          target=\"_blank\"\n          href=\"//plus.google.com/share?url={{../../url}}\"\n          class=\"share-google\"\n          data-share\n        >\n          <span class=\"sr-only\">Google Plus</span>\n          {{> svg/icon-google-plus}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'email'}}\n      <li class=\"share-link\">\n        <a\n          href=\"mailto:?subject={{../../title}}&amp;body={{../../url}}\"\n          class=\"share-email\"\n        >\n          <span class=\"sr-only\">{{lang 'common.email'}}</span>\n          {{> svg/icon-mail}}\n        </a>\n      </li>\n    {{/if}}\n\n    {{#if service '===' 'print'}}\n      <li class=\"share-link\">\n        <a class=\"print\" href=\"javascript:;\" onclick=\"window.print()\">\n          <span class=\"sr-only\">{{lang 'common.print'}}</span>\n          {{> svg/icon-print}}\n        </a>\n      </li>\n    {{/if}}\n\n  {{/each}}\n\n  {{#each share_buttons.buttons}}\n    {{#if service '===' 'facebook_like'}}\n      <li class=\"share-link facebook-like\">\n        <div class=\"fb-like\"\n          data-href=\"{{../../url}}\"\n          data-layout=\"button_count\"\n          data-action=\"like\"\n          data-show-faces=\"false\">\n        </div>\n      </li>\n    {{/if}}\n  {{/each}}\n</ul>\n",
