{
  "components/common/newsletter-form": "<div class=\"newsletter\">\n  <h2 class=\"newsletter-heading\">\n    {{lang 'newsletter.heading'}}\n  </h2>\n  <form\n    class=\"form newsletter-subscription-form\"\n    action=\"{{urls.subscribe.action}}\"\n    method=\"post\"\n    data-validated-form\n  >\n    <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n    <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n    <input type=\"hidden\" name=\"check\" value=\"1\">\n    <div class=\"form-field form-inline\">\n      <label\n        class=\"sr-only\"\n        for=\"nl_email\"\n      >\n        {{lang 'newsletter.email_placeholder'}}\n      </label>\n      <input\n        class=\"form-input newsletter-input\"\n        id=\"nl_email\"\n        name=\"nl_email\"\n        type=\"email\"\n        value=\"{{customer.email}}\"\n        placeholder=\"{{lang 'newsletter.email_placeholder'}}\"\n        required\n      >\n      <div class=\"newsletter-button-wrapper\">\n        <input\n          class=\"button button-primary button-small\"\n          type=\"submit\"\n          value=\"{{lang 'newsletter.submit'}}\"\n        >\n      </div>\n    </div>\n  </form>\n  {{#if settings.show_newsletter_summary}}\n    <div class=\"newsletter-summary\">{{settings.newsletter_summary}}</div>\n  {{/if}}\n</div>\n"
}
